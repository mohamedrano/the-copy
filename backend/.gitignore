# Backend (Express.js) specific gitignore

# Dependencies
node_modules/
/.pnp
.pnp.*

# Environment variables
.env
.env.local
.env.development
.env.production
.env.test

# Build output
/dist
/build

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
.nyc_output/

# TypeScript
*.tsbuildinfo

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Microbundle cache
.rpt2_cache/
.rts2_cache_cjs/
.rts2_cache_es/
.rts2_cache_umd/

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# Uploaded files
uploads/

# Database files
*.db
*.sqlite
*.sqlite3

# Misc
.DS_Store
*.pem

# PM2
.pm2/
